{
  "headers": [
    {
      "source": "/",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" },
        { "key": "Clear-Site-Data", "value": "\"cache\"" }
      ]
    },
    {
      "source": "/services/(.*)",
      "headers": [
